{
  "name": "vite-plugin-vue-wc-styles",
  "version": "0.2.2",
  "description": "vite plugin for CSS in web components",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./custom-element": {
      "import": "./dist/custom-element.js",
      "require": "./dist/custom-element.js",
      "types": "./dist/custom-element.d.ts"
    },
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "custom-element": [
        "dist/custom-element.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc"
  },
  "keywords": [],
  "author": "Pedro Porto",
  "license": "MIT",
  "peerDependencies": {
    "vite": "^2.9.9"
  },
  "engines": {
    "node": ">=14.16.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.25.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint": "7.32.0",
    "lint-staged": "12.3.5",
    "prettier": "2.5.1",
    "stylelint": "14.9.1",
    "typescript": "4.9.5",
    "vite": "^2.9.9",
    "@types/node": "^18.11.18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pedroporto/vite-plugin-vue-wc-styles.git"
  },
  "homepage": "https://github.com/pedroporto/vite-plugin-vue-wc-styles#readme"
}
